[user]
	name = Jack Rose
	email = jackderryrose@gmail.com
[alias]
	aa = add .
	br = branch
	ci = commit
	cia = commit -a
	co = checkout
	cob = checkout -b
	cod = checkout develop
	com = checkout master
	df = diff
	dfs = diff --staged
	st = status
	pop = stash pop
	unstage = reset HEAD --
	meld = !git difftool -t meld --dir-diff
[core]
	excludesfile = /home/jack/.globalgitignore
	pager = diff-so-fancy | less
[push]
	default = current

