{
  "let statement": {
    "prefix": "l",
    "body": "let ${1:name} = $2"
  },
  "const statement": {
    "prefix": "c",
    "body": "const ${1:name} = $2"
  },
  "multi-line comment": {
    "prefix": "/",
    "body": "/* $0 */"
  },
  "jsdoc": {
    "prefix": "jsdoc",
    "body": ["/**", " * $0", " */"]
  }
}
