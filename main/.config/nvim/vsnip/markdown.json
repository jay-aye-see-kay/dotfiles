{
  "task": {
    "prefix": "t",
    "body": ["- [ ] $0"]
  },
  "ordered list": {
    "prefix": "ol",
    "body": ["1. $0"]
  },
  "unordered list": {
    "prefix": "ul",
    "body": ["- $0"]
  },
  "code block": {
    "prefix": "c",
    "body": ["```$1", "$0", "```"]
  },
  "shell code block": {
    "prefix": "cs",
    "body": ["```sh", "$0", "```"]
  },
  "javascript code block": {
    "prefix": "cj",
    "body": ["```javascript", "$0", "```"]
  },
  "typescript code block": {
    "prefix": "ct",
    "body": ["```typescript", "$0", "```"]
  },
  "python code block": {
    "prefix": "cp",
    "body": ["```python", "$0", "```"]
  }
}
